{
    "name": "Urlify 短链接工具",
    "description": "快速生成短连接工具",
    "version": "1.0.2",
    "permissions": ["contextMenus", "https://urlify.cn/"],
    "background":
    {
        "scripts": ["urlify.js"]
    },
    "manifest_version": 2,
    "icons":
    {
        "128": "urlify.png"
    },
    "browser_action":
    {
        "default_icon": "urlify.png",
        "default_title": "Urlify 短链接工具"
    }
}